import java.util.*;
public class Solution {
    public static void main(String[] args) {
        try {
        Scanner sc = new Scanner(System.in);
        long B = sc.nextInt();
        long H = sc.nextInt();
        
        sc.close();
        if(B <= 0 || H <= 0) {
            throw new Exception("Breadth and height must be positive");
        }
        System.out.println(B*H);
        } catch (Exception e) {
            System.out.println(e);
        }
    }
}
